  // ËæÖÂä©ÔºöÊâãÊú∫/Âπ≥ÊùøËá™ÈÄÇÂ∫îÂ§ßÂ∞è
  private bigImageSize(): number {
    const dsp = display.getDefaultDisplaySync() as display.Display;
